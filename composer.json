{
	"name": "flarum-lang/asturian",
	"type": "flarum-extension",
	"description": "Asturian language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/asturian",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"ast-locale",
		"asturian-locale",
		"asturian"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/asturian/issues",
		"source": "https://github.com/flarum-lang/asturian"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Asturian",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "ast",
			"title": "Asturian"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
